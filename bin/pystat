#!/usr/local/bin/python3
import argparse
from sys import argv, stdin
from statistics import mean, median, variance, stdev


def main():
    data = list()
    for row in stdin.readlines():
        row = row.strip()
        try:
            data.append(float(row))
        except:
            if FORCE:
                pass
            else:
                print('error line: {}'.format(row))

    print('num_data     {}'.format(len(data)))
    print('min          {}'.format(min(data)))
    print('max          {}'.format(max(data)))
    print('mean         {}'.format(mean(data)))
    print('median       {}'.format(median(data)))
    print('variance     {}'.format(variance(data)))
    print('stdev        {}'.format(stdev(data)))


if __name__ == '__main__':
    description = """
        output mean value.
        if input line is not number, the line is ignore"""

    parser = argparse.ArgumentParser(
        description=description,
    )
    parser.add_argument(
        '-f', '--force', action='store_true',
        help='Do not show error message'
    )
    args = parser.parse_args()

    FORCE = args.force

    main()
