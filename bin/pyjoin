#!/usr/local/bin/python3
import argparse
from sys import stdin


def main(_str):
    for row in stdin.readlines():
        print(_str.join(row.split()))


if __name__ == '__main__':
    parser = argparse.ArgumentParser(
        description="join",
    )
    parser.add_argument('str')
    args = parser.parse_args()

    main(_str=args.str)
